{
  "name": "neat-dhcpd",
  "version": "0.0.0",
  "license": "ISC",
  "engines": {
    "node": "20.10.0",
    "pnpm": ">=8.14"
  },
  "private": true,
  "keywords": [
    "dhcpd"
  ],
  "scripts": {
    "dev": "concurrently --kill-others --prefix-colors auto pnpm:dev:*",
    "dev:db": "pnpm --filter \"@neat-dhcpd/db\" dev",
    "dev:common": "pnpm --filter \"@neat-dhcpd/common\" dev",
    "dev:dhcpd": "node start.waitOnDb.js && pnpm --filter \"@neat-dhcpd/dhcpd\" dev",
    "dev:web-ui": "node start.waitOnDb.js && pnpm --filter \"@neat-dhcpd/web-ui\" dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "start": "pnpm --filter \"@neat-dhcpd/db\" start:migrateToLatest && concurrently --kill-others --prefix-colors auto pnpm:start:*",
    "start:db": "pnpm --filter \"@neat-dhcpd/db\" start",
    "start:dhcpd": "node start.waitOnDb.js && pnpm --filter \"@neat-dhcpd/dhcpd\" start",
    "start:web-ui": "node start.waitOnDb.js && pnpm --filter \"@neat-dhcpd/web-ui\" start"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-functional": "6.0.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
